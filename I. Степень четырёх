def is_power_three(n):
    if n == 1:
        return 'True' #является исключением по условиям проверки
    if n < 1 or n%4!=0 : #если число меньше 1 или целочисленно не делится, то False
        return 'False'
    return is_power_three(n//4) #Целочисленное деление. Зацикливание. Первое цисло делится на 4. Потом результат снова прогоняется и делится на 4. И так до получения остатка 1
 
n = int(input())
print(is_power_three(n))
